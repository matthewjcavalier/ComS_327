    "version": "0.2.0",
    "configurations": [
        {
            "name": "convert",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/conversion/game",
            "args": ["--nummon","5"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Loading",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crawler",
            "args": ["--load"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
    {
        "name": "Normal",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/debug",
        "args": ["--nummon","5","-s", "1"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
    },
        {
            "name": "heap",
